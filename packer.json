  ],
  "post-processors": [
    [
      {
        "type": "vagrant",
        "output": "builds/packer_{{.BuildName}}_{{.Provider}}.box"
      },
      {
        "type": "vagrant-cloud",
        "box_tag": "jamesla/ubuntu-1604-min-daily",
        "access_token": "{{ user `vagrant_cloud_token` }}",
